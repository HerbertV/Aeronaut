									<xs:attribute name="portwingLeading" type="xs:int" use="required"/>
									<xs:attribute name="portwingTrailing" type="xs:int" use="required"/>
									<xs:attribute name="starboardwingLeading" type="xs:int" use="required"/>
									<xs:attribute name="starboardwingTrailing" type="xs:int" use="required"/>
									<xs:attribute name="tail" type="xs:int" use="required"/>
